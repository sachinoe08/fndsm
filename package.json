    "@fortawesome/react-fontawesome": "^0.1.16",
    "ahooks": "^3.1.9",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "constate": "^3.3.0",
    "currency.js": "^2.0.4",
    "html-react-parser": "^1.4.8",
    "minimist": "^1.2.5",
    "rc-slider": "^9.7.5",
    "react": "^17.0.2",
