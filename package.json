{
  "name": "@fn/fn-ds",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build-css": "sass --no-source-map --load-path=./node_modules ./src:./dist",
    "format": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "types:check": "tsc --pretty --noEmit",
    "lint": "eslint ./src --ext ts --ext tsx --ext js",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "build": "node scripts/pre-build && tsc --build && npm run build-css && node scripts/create-package",
    "artifactregistry-login": "npx google-artifactregistry-auth",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel ./src -d dist --copy-files",
    "delete-maps": "npm run delete-map-files && npm run delete-references-to-map-files",
    "delete-map-files": "find ./dist -name '*.map' -delete",
    "delete-references-to-map-files": "find ./dist -regex '.*\\.\\(|\\js\\|css\\|ts\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js|ts)\\.map(\\*\\/)?/g' {} +",
    "prepare": "husky install",
    "clean": "rimraf ./dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@fortawesome/fontawesome-common-types": "^0.3.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@types/react": "^17.0.36",
    "@types/react-bootstrap": "^0.32.28",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-copy": "^4.0.1",
    "gulp-inject-string": "^1.1.2",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-shell": "^0.8.0",
    "husky": "7.0.4",
    "ignore-emit-webpack-plugin": "^2.0.6",
    "install": "^0.13.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm": "^8.5.1",
    "prettier": "2.5.1",
    "replace-in-file": "^6.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.6.0",
